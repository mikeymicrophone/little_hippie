<h1>Listing coupon_designs</h1>

<table>
  <tr>
    <th>Coupon</th>
    <th>Design</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @coupon_designs.each do |coupon_design| %>
  <tr>
    <td><%= link_to_name coupon_design.coupon %></td>
    <td><%= link_to_name coupon_design.design %></td>
    <td><%= link_to 'Show', coupon_design %></td>
    <td><%= link_to 'Edit', edit_coupon_design_path(coupon_design) %></td>
    <td><%= link_to 'Destroy', coupon_design, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

