<% if charge.cart.andand.tracking_number.present? %>
  <%= link_to charge.cart.tracking_number, edit_cart_path(charge.cart) %>
<% end %>
<% if charge.cart && charge.shippable? %>
  <%= form_for charge.cart, :remote => true do |f| %>
    <%= f.text_field :tracking_number, :value => nil %>
    <%= f.submit :enter %>
  <% end %>
<% end %>
