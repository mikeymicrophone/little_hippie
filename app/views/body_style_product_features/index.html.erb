<h1>Listing body_style_product_features</h1>

<table>
  <tr>
    <th>Body style</th>
    <th>Product color</th>
    <th>Position</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @body_style_product_features.each do |body_style_product_feature| %>
  <tr>
    <td><%= link_to_name body_style_product_feature.body_style %></td>
    <td><%= link_to_name body_style_product_feature.product_color %></td>
    <td><%= body_style_product_feature.position %></td>
    <td><%= link_to image_tag(asset_path('up_button.gif')), move_up_body_style_product_feature_path(body_style_product_feature), :method => :put %></td>
    <td><%= link_to image_tag(asset_path('down_button.gif')), move_down_body_style_product_feature_path(body_style_product_feature), :method => :put %></td>
    <td><%= link_to 'Show', body_style_product_feature %></td>
    <td><%= link_to 'Edit', edit_body_style_product_feature_path(body_style_product_feature) %></td>
    <td><%= link_to 'Destroy', body_style_product_feature, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

