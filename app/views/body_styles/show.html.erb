<p>
  <b>Name:</b>
  <%= @body_style.name %>
</p>

<p>
  <b>Code:</b>
  <%= @body_style.code %>
</p>

<%= link_to 'Edit', edit_body_style_path(@body_style) %> |
<%= link_to 'Back', body_styles_path %>
<% @products.each do |product| %>
  <%= div_for product do %>
    <%= image_tag product.primary_image, :width => 50 %>
    <%= link_to_name product %>
  <% end %>
<% end %>