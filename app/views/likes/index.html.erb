<h1>Listing likes</h1>

<table>
  <tr>
    <th>Product</th>
    <th>Customer</th>
    <th>Ip</th>
    <th>Cart</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @likes.each do |like| %>
  <tr>
    <td><%= like.product %></td>
    <td><%= like.customer %></td>
    <td><%= like.ip %></td>
    <td><%= like.cart %></td>
    <td><%= link_to 'Show', like %></td>
    <td><%= link_to 'Edit', edit_like_path(like) %></td>
    <td><%= link_to 'Destroy', like, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Like', new_like_path %>
